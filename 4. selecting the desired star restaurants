browser.maximize_window()
time.sleep(10)
browser.find_element(By.XPATH, '//body/jsl/div/div/div/div/div/div/div/div/div[3]/div[1]/button[1]').click()
time.sleep(10)
browser.find_element(By.XPATH, '//body//div//div//div//div//div//div//div//button//span[1]').click()

time.sleep(10)
from selenium.webdriver.common.action_chains import ActionChains
ActionChains(browser) \
    .key_down(Keys.ARROW_DOWN) \
    .key_up(Keys.ARROW_DOWN) \
    .key_down(Keys.ARROW_DOWN) \
    .key_up(Keys.ARROW_DOWN) \
    .key_down(Keys.ARROW_DOWN) \
    .key_up(Keys.ARROW_DOWN) \
    .key_down(Keys.ARROW_DOWN) \
    .key_up(Keys.ARROW_DOWN) \
    .key_down(Keys.ARROW_DOWN) \
    .key_up(Keys.ARROW_DOWN) \
    .key_down(Keys.ARROW_DOWN) \
    .key_up(Keys.ARROW_DOWN) \
    .key_down(Keys.ENTER) \
    .key_up(Keys.ENTER) \
    .perform()

time.sleep(10)
